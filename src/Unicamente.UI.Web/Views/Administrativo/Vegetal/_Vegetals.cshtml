@model Unicamente.UI.Web.Models.AdministrativoViewModel

<section class="realstate" style="padding: 0">
    <div class="container">

        <div class="section-title" style="padding: 5px">
            <h2>Cadastro de Vegetal</h2>

        </div>

        <div id="tabela-vegetal">

            <partial name="_ListaVegetals.cshtml" model="@Model.Vegetals" />
        </div>

        <div id="formulario-vegetal" style="display:none">
            <partial name="_FormularioVegetal.cshtml" model="@Model.Vegetal" />
        </div>

    </div>


</section>
@* <script src="~/js/jquery.min.js"></script>
