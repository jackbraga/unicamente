<script>



    function atualizaListaVegetals() {
        $.ajax({
            type: 'GET',
            url: '/Administrativo/ListaVegetals',
            // dataType: 'html',
            success: function (data) {
                $("#tabela-vegetal").html('');
                $("#tabela-vegetal").html(data);
                tabelaVegetal();

            }
        });
    }

    function tabelaVegetal() {
        $("#tabela-vegetal").show();
        $("#formulario-vegetal").hide();
        limpaFormularioVegetal();
    }

    function formularioVegetal() {
        $("#tabela-vegetal").hide();
        $("#formulario-vegetal").show();

    }


</script>